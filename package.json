{
  "name": "javascript-project-template",
  "version": "1.0.0",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "prepare": "husky",
    "start": "node .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretty": "prettier --check .",
    "pretty:fix": "prettier --write .",
    "test": "c8 node --test",
    "test:watch": "c8 node --test --watch",
    "check-unused-dependencies": "depcheck",
    "check-circular-dependencies": "madge --circular ./src ./test"
  },
  "devDependencies": {
    "c8": "^10.1.3",
    "depcheck": "^1.4.3",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "madge": "^8.0.0",
    "prettier": "^3.6.2"
  }
}
